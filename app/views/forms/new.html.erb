

<%= image_tag("sleepy.jpg", class: "img-responsive",:style=>"width :100%, position :top center;", :alt => "sleepy") %>

<div class="container-fluid">
<!--Contact Form-->
<div class="new-form"></div>
	<div class="col-md-6 contact">
		<h1> Say hello here! </h1>
		<br>
		<%= simple_form_for @form do |f| %>
  		<%= f.input :name %>
  		<%= f.input :email %>
  		<%= f.input :message %>
  		<%= f.button :submit, 'Submit' %>
		<% end %>
	</div> 
<!--end-->

	<div class="col-md-5 ">

	<!--Social Media-->

		<div class="social-border">

		<%= link_to image_tag("github2.png", class: "img-responsive social", :alt => "github"), "http://www.github.com/shirlaine" %>

		<%= link_to image_tag("facebook2.png", class: "img-responsive social2", :alt => "github"), "http://www.facebook.com/shirlaine.phang" %>

		</div>
		
	<!--end-->

	</div>


</div>

<div id="contact-bottom"></div>