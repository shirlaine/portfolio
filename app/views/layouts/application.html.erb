        <% end %>
    

     </div>
      <ul>
        <li class="category">About Me (static PAge) </li>
       <li class="category">Contact Me (form) </li>
        <li class="category">Video static + youtube</li>
        <li><%= link_to "Blog", root_path %> </li>
        <li><a href="http://localhost:3000/posts">Blog</a></li>
  
    
    
      

    </ul>


    </div>  
  
    <%= yield %>
